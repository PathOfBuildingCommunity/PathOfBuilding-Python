if [ ! -s src/PoB_Main_Window.py ]
then
	pyside6-uic Assets/PoB_Main_Window.ui -o src/PoB_Main_Window.py
fi
if [ ! -s src/PoB_rc.py ]
then
	pyside6-rcc Assets/PoB.qrc -o src/PoB_rc.py
fi
# Change directories as settings.xml is created in the current directory. TreeData is also referenced from there.
cd src
python PathOfBuilding.py 
cd ..
